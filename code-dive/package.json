{
  "name": "code-dive",
  "displayName": "Code Dive",
  "description": "Code Dive is a VS Code Extension that serves as a Visual Programming Tool for Textual Based Programming Languages.",
  "publisher": "bdaniel",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "activationEvents": [
    "onCommand:extension.code-dive"
  ],
  "main": "./build/src-ext/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.code-dive",
        "title": "Start Code Dive",
        "category": "Code Dive"
      }
    ]
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "terser": "3.16.0",
    "vscode": "^1.1.17"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "vscode:prepublish": "node ./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts start",
    "build": "node ./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "react-scripts": "^2.1.3",
    "rewire": "^4.0.1",
    "typescript": "^3.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
