{
  "name": "code-dive",
  "displayName": "Code Dive",
  "description": "Code Dive is a VS Code Extension that serves as a Visual Programming Tool for Textual Based Programming Languages.",
  "publisher": "bdaniel",
  "version": "0.1.0",
  "engines": {
    "vscode": "^1.23.0"
  },
  "activationEvents": [
    "onCommand:extension.code-dive"
  ],
  "main": "./build/src-ext/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.code-dive",
        "title": "Start Code Dive",
        "category": "Code Dive"
      },
      {
        "command": "extension.code-says-hello",
        "title": "Code Dive Says Hello",
        "category": "Code Dive"
      },
      {
        "command": "extension.code-dive-analysis",
        "title": "Code Dive Analysis",
        "category": "Code Dive"
      }
    ]
  },
  "dependencies": {
    "@emotion/styled": "^10.0.27",
    "@types/react-redux": "^7.1.7",
    "jison": "^0.4.18",
    "paths-js": "^0.4.10",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "resize-observer-polyfill": "^1.5.1",
    "terser": "3.16.0",
    "vscode": "^1.1.17"
  },
  "resolutions": {
    "terser": "3.14.1"
  },
  "scripts": {
    "vscode:prepublish": "yarn build-webview && build-extension",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "start": "react-scripts start",
    "build": "yarn build-webview && yarn build-extension",
    "build-webview": "node ./scripts/build-non-split.js",
    "build-extension": "tsc -p tsconfig.extension.json && xcopy src-ext\\jison build\\src-ext\\jison /i /y",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/jest": "^23.3.13",
    "@types/node": "^10.1.2",
    "@types/react": "^16.3.14",
    "@types/react-dom": "^16.0.5",
    "react-scripts": "^2.1.3",
    "rewire": "^4.0.1",
    "typescript": "^3.3.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
